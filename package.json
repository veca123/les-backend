{
  "name": "les-backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --exit-child --ignore-watch node_modules --respawn src/shared/infra/server.ts",
    "pm": "prisma migrate dev",
    "pd": "prisma migrate deploy",
    "pg": "prisma generate",
    "ps": "prisma studio",
    "build": "tsc --project tsconfig.json  && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "postinstall": "npm run build",
    "start": "node dist/shared/infra/server.js",
    "heroku-postbuild": "npm run prisma migrate dev"
  },
  "dependencies": {
    "@prisma/client": "^4.1.1",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "csv-parse": "^5.2.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.4.0",
    "tsyringe": "^4.7.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.14",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.7.1",
    "prisma": "^4.1.1",
    "supertest": "^6.2.4",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.7.4"
  }
